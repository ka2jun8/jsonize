{
    "name": "Jsonize",
    "manifest_version": 2,
    "version": "1.0",
    "description": "Table data to json",
    "icons": {
        "128": "resources/icon128.png"
    },
    "browser_action": {
        "default_title": "Jsonize",
        "default_popup": "index.html",
        "default_icon": "resources/icon128.png"
    },
    "permission": [
        "activeTab",
        "http://*/*",
        "https://*/*"
    ],
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [
                "jsonize.js"
            ],
            "run_at": "document_start"
        }
    ]
}